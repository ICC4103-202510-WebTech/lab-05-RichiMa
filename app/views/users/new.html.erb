<h1> New User </h1>

<%= form_with(model: @user) do |f| %>
    <div>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
    </div>

    <div>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
    </div>

    <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
    </div>

    <%= f.submit 'Create User' %>
    <% end %>
<%= link_to 'Back', users_path%>
